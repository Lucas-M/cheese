<br>
<div class="btn-group" role="group" aria-label="Basic example">
  <%= link_to 'Home', "/greetings/hello", :class => "btn btn-primary", :style=>"color:#FFFFFF;" %>
  <%= link_to 'Cheeses', "/items", :class => "btn btn-primary", :style=>"color:#FFFFFF;" %>
</div>

<p id="notice"><%= notice %></p>

<h1>Cheese List</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.content %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
